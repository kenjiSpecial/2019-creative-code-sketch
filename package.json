{
  "name": "paracel-prtotype-kit",
  "version": "1.0.0",
  "description": "prototype tool with parcel",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "01": "parcel src/01-typography/index.html --no-hmr --port 1234  --out-dir sketches/01-typography",
    "02": "parcel src/02-color/index.html --no-hmr --port 1234  --out-dir sketches/02-color",
    "04": "parcel src/04-line/index.html --no-hmr --port 1234  --out-dir sketches/04-line",
    "d01": "rm -rf sketches/01-typography/*.js && parcel build src/01-typography/index.html --out-dir sketches/01-typography --public-url ./  --no-source-maps",
    "d02": "rm -rf sketches/02-color/*.js && rm -rf sketches/02-color/*.js.map && parcel build src/02-color/index.html --out-dir sketches/02-color --public-url ./  --no-source-maps",
    "d04": "rm -rf sketches/04-line/*.js && rm -rf sketches/04-line/*.js.map && parcel build src/04-line/index.html --out-dir sketches/02-color --public-url ./  --no-source-maps",
    "dist": "python -m SimpleHTTPServer 8200"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^0.19.0",
    "color-scheme": "^1.0.1",
    "dat.gui": "^0.7.6",
    "firebase": "^7.2.2",
    "gsap": "^2.1.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "stats": "^1.0.0",
    "stats.js": "^0.17.0",
    "three": "^0.109.0",
    "three-orbit-controls": "^82.1.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-redux": "^7.1.5",
    "@types/redux-logger": "^3.0.7",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "parcel-bundler": "^1.11.0",
    "typescript": "^3.6.4"
  }
}
